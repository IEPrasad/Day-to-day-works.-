def fib():
  a,b = 0,1
  while True:
    c = a+b
    yield a 
    a,b = b,c

y = fib()
print(next(y))
print(next(y))
print(next(y))
print(next(y))
print(next(y))
print(next(y))
print(next(y))
print(next(y))
print(next(y))

## the out put is 
  #>>> 
0
1
1
2
3
5
8
13
21

## this is how to get fibonaci sequence with python generators 
## try to do this with another way 
